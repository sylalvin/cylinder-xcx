.weui-cells{
  margin-top: 0;
}
.webox{
  display: block;
}
.checkbox{
  display: flex;
  flex-wrap: wrap;
  flex-direction: row;
  justify-content: space-between;
  width: 100%;
}
.checkbox text{
  text-align: left;
  flex: 2;
}
.checkbox checkbox{
  text-align: right;
  flex: 1;
  padding-right: 40rpx;
}
.radio-group{
  text-align: center;
}
.radio{
  margin: auto 20px !important;
}
.pa{
  display: block;
  text-align: center;
}
.db{
  display: block !important;
}
.fl{
  float: left;
}
.fr{
  float: right;
}
.w80{
  width: 80%;
  float: left;
}
.w20{
  width: 20%;
  float: right;
}
.item-text{
  padding: 5px 20px;
  font-size: 20px;
  border-top: .5px solid #ccc;
}
.weui-vcode-btn {
  display: inline-block;
  padding: 0 1rem;
  border-left: 1px solid rgba(0,0,0,.1);
  line-height: 3.29411765em;
  font-size: 17px;
  white-space: nowrap;
  margin-left: 5px;
  height: 3.29411765em;
  vertical-align: middle;
}