<view class="page">
    <view class="page__bd">

        <view class="weui-cells">
            <view class="weui-cell pa">
                <view style="display: inline-block; vertical-align: middle;text-align: center;">扫描：{{scan_number}} 散瓶：{{scan_bulk}} 集格：{{scan_set}} 总气瓶数：{{scan_sum}}
                </view>
            </view>
        </view>

        <view class="weui-cells"  wx:for="{{allCylinderList}}" wx:key="cylinderNumber">
            <view class="weui-cell flex">
                <text class="f4">
                    <text class="w100">所属集格：{{item.setId}}\n</text>
                    <text class="w100">气瓶二维码编号：{{item.cylinderNumber}}</text>
                </text>
                <button class="f1" type="default" bindtap="deleteCylinder" data-set-id="{{item.setId}}" data-cylinder-number="{{item.cylinderNumber}}">删 除</button>
            </view>
        </view>

    </view>
</view>