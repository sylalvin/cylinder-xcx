.navigator-hover{
  height: 46px;
}
.weui-cells{
  margin-top: 0;
}
.webox{
  display: block;
}
.checkbox{
  display: flex;
  flex-wrap: wrap;
  flex-direction: row;
  justify-content: space-between;
  width: 100%;
}
.checkbox text{
  text-align: left;
  flex: 2;
}
.checkbox checkbox{
  text-align: right;
  flex: 1;
  padding-right: 40rpx;
}
.radio-group{
  text-align: center;
}
.radio{
  margin: auto 20px !important;
}
.pa{
  display: block;
  text-align: center;
}
.db{
  display: block !important;
}
.fl{
  float: left;
}
.fr{
  float: right;
}
.w80{
  width: 80%;
  float: left;
}
.w20{
  width: 20%;
  float: right;
}
.item-text{
  padding: 5px 20px;
  font-size: 20px;
  border-top: .5px solid #ccc;
}
.weui-vcode-btn {
  display: inline-block;
  padding: 0 1rem;
  border-left: 1px solid rgba(0,0,0,.1);
  line-height: 3.29411765em;
  font-size: 17px;
  white-space: nowrap;
  margin-left: 5px;
  height: 3.29411765em;
  vertical-align: middle;
}
.mask {
  display: none;
  position: absolute;
  top: 0%;
  left: 0%;
  width: 100%;
  height: 100%;
  z-index: 100;
  opacity: 0.3;
}
.camera{
  width: 310px;
  height: 310px;
  position: absolute;
  top: 70px;
  left: calc(50% - 155px);
}
.out-view{
  width: 310px;
  height: 310px;
  border: 5px dashed #ccc;
  margin: 20px auto;
  position: relative;
}
.all-border{
  width: 20px;
  height: 20px;
  position: absolute;
}
.top-left{
  top: -5px;
  left: -5px;
  border-top: 5px solid rgb(20, 117, 28);
  border-left: 5px solid rgb(20, 117, 28)
}
.top-right{
  top: -5px;
  left: 290px;
  border-top: 5px solid rgb(20, 117, 28);
  border-right: 5px solid rgb(20, 117, 28)
}
.bottom-left{
  top: 290px;
  left: -5px;
  border-left: 5px solid rgb(20, 117, 28);
  border-bottom: 5px solid rgb(20, 117, 28)
}
.bottom-right{
  top: 290px;
  left: 290px;
  border-bottom: 5px solid rgb(20, 117, 28);
  border-right: 5px solid rgb(20, 117, 28)
}